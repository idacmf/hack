<% if !notice.nil? %>
  <div id="alertnotice" class="row">
    <div id="noticealert" class="col-xs-12">
      <p class="notice"><%= notice %></p>
    </div>
  </div>
<% end %>
<% if !alert.nil? %>
  <div id="alertnotice" class="row">
    <div id="noticealert" class="col-xs-12">
      <p class="alert"><%= alert %></p>
    </div>
  </div>
<% end %>