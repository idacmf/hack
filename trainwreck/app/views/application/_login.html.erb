<div class="row" id="login_holder">
  <% if user_signed_in? %>
      <div id="login_empty" class="col-xs-9"></div>
      <div id="my_acc" class="col-xs-2">
        <%= link_to("My Account", edit_user_registration_path) %>
      </div>
      <div id="my_acc" class="col-xs-1">
        <%= link_to("Sign Out", destroy_user_session_path, method: :delete) %>
      </div>
  <% else %>
      <div id="login_empty" class="col-xs-11"></div>
      <div id="login" class="col-xs-1"><%= link_to("Log In", new_user_session_path) %></div>
  <% end %>
</div>
